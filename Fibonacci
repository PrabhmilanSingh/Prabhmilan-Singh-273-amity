#include <stdio.h>

int fibo(int n)
{
    if(n==0)
    return 0;
    
    else if(n==1)
    return 1;
    
    else
    
    return fibo(n-1)+ fibo(n-2);
}
   
int main()
{
    int n,d;
    printf("\n enter the number of term");
    scanf("%d",&n);
    
    d= fibo(n);
    printf("%d",d);
    
    return 0;
}
